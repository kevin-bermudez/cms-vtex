{
	"source" : {
		"include" : "./libs",
		"includePattern" : ".js$",
		"excludePatterns" : "(node_modules/|docs)"
	},
	"plugins" : [
		"plugins/markdown"
	],
	"opts" : {
		"encoding" : "utf8",
		"readme" : "./readme.md",
		"destination" : "docs/",
		"recurse" : true,
		"verbose" : true,
		"template": "./node_modules/jsdoc-template"
	}	
}